{
    "version": 2,
    "name": "disqusjs-proxy",
    "routes": [
        {
            "src": "/uploads/(.*)",
            "headers": {
                "cache-control": "public, max-age=[32]"
            },
            "dest": "https://c.disquscdn.com/uploads/$1"
        },
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "public, max-age=[32]"
            },
            "dest": "https://disqus.com/api/$1"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}
